{
  "manifest_version": 3,
  "name": "MOOdle Unizar Personalizado",
  "version": "1.3.1",
  "description": "Personaliza el aspecto de Moodle Unizar autom√°ticamente",
    "action": {
    "default_icon": {
      "16": "icon-16.png",
      "32": "icon-32.png",
      "48": "icon-48.png",
      "128": "icon-128.png"
    }
  },
  "content_scripts": [
    {
      "matches": ["https://moodle.unizar.es/*"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "moodle-unizar-personalizado@carmoran",
      "update_url": "https://carmoran0.github.io/MOOdleUnizarCSS/updates.json"
    }
  }
}
